{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ecs_component_registry",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "component"
      }
    },
    "component": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "component_name"
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "component_data"
              },
              {
                "type": "STRING",
                "value": "\n"
              }
            ]
          }
        }
      ]
    },
    "component_data": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 10,
          "content": {
            "type": "SYMBOL",
            "name": "tag"
          }
        },
        {
          "type": "SYMBOL",
          "name": "field"
        }
      ]
    },
    "component_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "tag_storage"
            },
            {
              "type": "SYMBOL",
              "name": "tag_storage_limit"
            }
          ]
        }
      ]
    },
    "tag_storage": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "storage"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "tag_storage_value"
        }
      ]
    },
    "tag_storage_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "dense"
        },
        {
          "type": "STRING",
          "value": "sparse"
        },
        {
          "type": "STRING",
          "value": "flag"
        }
      ]
    },
    "tag_storage_limit": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "limit"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "field": {
      "type": "PATTERN",
      "value": "[^\\r\\n\\[@]+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]+"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": "\r"
    },
    {
      "type": "STRING",
      "value": "\t"
    },
    {
      "type": "STRING",
      "value": " "
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}